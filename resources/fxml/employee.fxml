<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane hgap="5.0" prefHeight="354.0" prefWidth="359.0" vgap="5.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <stylesheets>
      <URL value="@/css/users.css" />
   </stylesheets>
   <columnConstraints>
      <ColumnConstraints hgrow="NEVER" maxWidth="110.0" minWidth="70.0" prefWidth="110.0" />
      <ColumnConstraints hgrow="ALWAYS" maxWidth="207.0" minWidth="10.0" prefWidth="162.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints vgrow="NEVER" />
      <RowConstraints vgrow="NEVER" />
      <RowConstraints vgrow="NEVER" />
      <RowConstraints vgrow="NEVER" />
      <RowConstraints vgrow="NEVER" />
      <RowConstraints vgrow="SOMETIMES" />
      <RowConstraints vgrow="SOMETIMES" />
      <RowConstraints vgrow="SOMETIMES" />
      <RowConstraints vgrow="SOMETIMES" />
      <RowConstraints vgrow="NEVER" />
      <RowConstraints vgrow="NEVER" />
      <RowConstraints vgrow="SOMETIMES" />
      <RowConstraints vgrow="SOMETIMES" />
      <RowConstraints vgrow="SOMETIMES" />
      <RowConstraints vgrow="SOMETIMES" />
      <RowConstraints vgrow="SOMETIMES" />
      <RowConstraints vgrow="NEVER" />
   </rowConstraints>
   <children>
      <Label text="%first_name" />
      <Label text="%last_name" GridPane.rowIndex="1" />
      <TextField fx:id="fldFirstName" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.vgrow="NEVER" />
      <TextField fx:id="fldLastName" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.vgrow="NEVER" />
      <Label text="%password" GridPane.rowIndex="2" />
      <Label text="%username" GridPane.rowIndex="3" />
      <TextField fx:id="fldUserName" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3" GridPane.vgrow="NEVER" />
      <DatePicker fx:id="fldBirthDate" prefHeight="25.0" prefWidth="210.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="9" GridPane.vgrow="NEVER" />
      <Label text="%birth_date" GridPane.rowIndex="9" />
      <Label text="%hire_date" GridPane.rowIndex="10" />
      <DatePicker fx:id="fldHireDate" prefHeight="25.0" prefWidth="209.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="10" GridPane.vgrow="NEVER" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0" GridPane.columnSpan="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="16" GridPane.vgrow="NEVER">
        <buttons>
          <Button fx:id="btnOk" mnemonicParsing="false" onAction="#clickOk" text="Ok" />
            <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#clickCancel" text="Cancel" />
        </buttons>
      </ButtonBar>
      <Separator prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="12" />
      <Label fx:id="takenUserName" text="%taken" textFill="#e57373" GridPane.columnIndex="1" GridPane.rowIndex="4">
         <font>
            <Font size="10.0" />
         </font></Label>
      <Separator prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="7" />
      <PasswordField fx:id="fldPassword" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" GridPane.vgrow="NEVER" />
      <RadioButton fx:id="admin" mnemonicParsing="false" prefHeight="17.0" prefWidth="207.0" text="Admin" GridPane.columnIndex="1" GridPane.rowIndex="6" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <opaqueInsets>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </opaqueInsets>
</GridPane>
